%% Make appendices have entry in the table of contents
\newcommand{\starredchapter}[1]{
  \clearpage
  %% Make spacing between top of page and heading less.
  \vspace*{-40\p@}
  \phantomsection
  %% Add the appendix name to the table of contents.
  \addcontentsline{toc}{chapter}{#1}\chapter*{#1}
}

%% Make English abstract have entry in the table of contents
\newcommand{\abstract}{
  \clearpage
  %% Make spacing between top of page and heading less.
  \vspace*{-40\p@}
  \phantomsection
  %% Add the appendix name to the table of contents.
  \addcontentsline{toc}{section}{ABSTRACT}\nonumsection*{ABSTRACT}
}

%% Make French abstract have entry in the table of contents
\newcommand{\frenchabstract}[1]{
  \clearpage
  %% Make spacing between top of page and heading less.
  \vspace*{-30\p@}
  \phantomsection
  \begin{center}
    \Large \textbf{#1}
  \end{center}
  \vspace{-6mm}
  %% Add the appendix name to the table of contents.
  \addcontentsline{toc}{section}{R\'ESUM\'E}\nonumsection*{R\'ESUM\'E}
}

